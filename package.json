  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/QianJue-CN/mcp-memory-server.git"
  },
  "bugs": {
    "url": "https://github.com/QianJue-CN/mcp-memory-server/issues"
